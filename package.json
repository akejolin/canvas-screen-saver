    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/snowpack-env": "^2.3.0",
    "prettier": "^2.0.5",
    "snowpack": "^2.9.0",
    "snowpack-plugin-baseurl-handler": "0.0.3",
    "snowpack-plugin-content-hash": "^1.1.0",
    "snowpack-plugin-sass": "^1.0.0",
    "typescript": "^3.9.7"
